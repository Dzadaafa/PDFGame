<< /S /JavaScript /JS (

var f = this.getField('collectable')
var ra = f.rect;
var val = f.value;
var gapY = ra[1] - ra[3];
var gapX = ra[2] - ra[0];
var i = ra[3];
    
function VAnimate(maks) {
    
    if (i <= maks) {

      var score = this.getField('score-point')

      if (isCollide()){
         f.value = "";
        var newX = randomize(100, 400);
        let randomized = [newX, ra[1], newX + gapX, ra[3]]; 
        f.rect = randomized;
        ra = randomized;
        f.value = val; 
        i = ra[3]; 
        score.value = parseInt(score.value) + 1
      } else {
         app.alert(`Game Over\nYour score: ${score.value}\n\nRefresh to restart`)
         return;
      }
        
    }

    let newRect = [ra[0], i + step + gapY, ra[2], i + step];
    f.value = "";
    f.rect = newRect;
    f.value = val;
    i -= step;

    app.setTimeOut("VAnimate(" + maks + ")", tick);
}


const hideEm = ["start-btn", "start-text", "start-bg"]
hideAll(hideEm);

app.setTimeOut(`VAnimate(${270})`, 1000)

) >>
endobj

9 0 obj
<< >>
stream

var step = 15;
var tick = 200;

function randomize(minX, maxX) {
    let baseValue = Math.floor(Math.random() * (maxX - minX + 1)) + minX;
    
    return baseValue
}

function hideAll(idItems) {
   for (const id of idItems) {
      const field = this.getField(id);
      if (field) {
         field.hidden = true
         field.readonly = true
         field.rect = [0, 0, 1, 1]
      }
   }
}

function HAnimate(maks, left=false) {
    var f = this.getField('basket')
    var ra = f.rect;
    var val = f.value;
    var gapY = ra[1] - ra[3];
    var gapX = ra[2] - ra[0];
    var i = !left? ra[0] : ra[2]
    
    let newRect = !left
                    ? [i + step, ra[1], i + step + gapX, ra[3]] 
                    : [i - step - gapX, ra[1], i - step, ra[3]];
    f.value = "";
    f.rect = newRect;
    f.value = val;
}


function isCollide(){
   const movObj = this.getField('collectable')
   const solidObj = this.getField('basket')
   var r1 = movObj.rect
   var r2 = solidObj.rect

   let condition = !(r1[2] < r2[0] || r1[0] > r2[2] || r1[1] < r2[3] || r1[3] > r2[1])

   return condition;
}

endstream
endobj

